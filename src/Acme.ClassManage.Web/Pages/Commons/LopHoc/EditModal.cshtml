@page
@using Acme.ClassManage.Localization
@using Acme.ClassManage.Web.Pages.Commons.LopHoc
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EditModal
@inject IStringLocalizer<ClassManageResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="LopHoc" asp-page="Commons/LopHoc/EditModal">
    <abp-modal>
        <abp-modal-header title="Update LopHoc"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
